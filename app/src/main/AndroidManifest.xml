<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qingsongjia.qingsongjia">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".activity.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".driverschool.SchoolListActivity" />
        <activity
            android:name=".activity.LoginActivity"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.RegisterActivity"
            android:label="@string/title_activity_register"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.VerifyPhoneActivity"
            android:label="@string/title_activity_verify_phone"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.SetPasswordActivity"
            android:label="@string/title_activity_set_password"
            android:theme="@style/AppTheme" />
        <activity android:name=".user.ChangeExamLibsActivity" />
        <activity
            android:name=".driverexam.ExamDetailActivity"
            android:label="@string/title_activity_exam_detail"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".driverexam.AnalogyExamActivity"
            android:label="@string/title_activity_analogy_exam"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".driverexam.InquiryExamActivity"
            android:label="@string/title_activity_inquiry_exam"
            android:theme="@style/AppTheme" />
        <!--
             ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".driverexam.AnalogyTestActivity"
            android:label="@string/title_activity_analogy_test"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".driverexam.ExamTestActivity"
            android:label="@string/title_activity_exam_test"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>
